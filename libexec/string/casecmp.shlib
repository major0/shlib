#!/usr/bin/env

import string/toupper

__shlib_string_casecmp() { test "$(string.toupper "${1}")" = "$(string.toupper "${2}")"; }
alias string.casecmp='__shlib_string_casecmp'

shlib.main {
	for arg; do
		case "${arg}" in (-h|-help|--help)
			echo "usage: ${0} <string1> <string2>"
			exit 0
		esac
	done
	string.casecmp "${@}"
}

